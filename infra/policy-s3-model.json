{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListModelPrefix",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::sound-realty-mlops-models-736296213166"],
      "Condition": { "StringLike": { "s3:prefix": [ "model/*", "model/" ] } }
    },
    {
      "Sid": "GetModelObjects",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": ["arn:aws:s3:::sound-realty-mlops-models-736296213166/model/*"]
    }
  ]
}
